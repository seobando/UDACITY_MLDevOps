name: test_regression_model
conda_env: conda.yml

entry_points:
  main:
    parameters:

      mlflow_model:
        description: Model to evaluate
        type: string

       test_dataset:
        description: Test data
        type: string


    command: >-
        python run.py  --mlflow_model {mlflow_model}  \
                       -- test_dataset { test_dataset} 
